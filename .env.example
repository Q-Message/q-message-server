# Variables de Entorno Necesarias

# Clave secreta para JWT (mínimo 32 caracteres, generada aleatoriamente)
JWT_SECRET=change_this_to_a_secure_random_string_min_32_chars

# Configuración de Base de Datos PostgreSQL
DB_USER=postgres
DB_PASSWORD=your_database_password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=q_message

# CORS - Orígenes permitidos (usar * en desarrollo, dominio específico en producción)
CORS_ORIGIN=*

# Puerto del servidor
PORT=3000

# Configuración de Email (Resend API)
RESEND_API_KEY=your_resend_api_key_here
